<window style="watch_04" full_screen="false" self_layout="default(x=205,y=45,w=390,h=390)" children_layout="default(r=1,c=1)" theme="watch_04_styles">
  <gesture name="gesture" x="c" y="m" w="100%" h="100%">
    <guage x="c" y="m" w="100%" h="100%" image="ico_watch_background_04">
      <button name="watch_04_day" style="enable" enable_long_press="true" x="89" y="125" w="88" h="88">
        <label name="week" style="week" x="c" y="23" w="43" h="16" text="WEN"/>
        <label name="day" style="day" x="c" y="48" w="25" h="22" text="10"/>
      </button>
      <button name="watch_04_weather" style="enable" enable_long_press="true" x="215" y="125" w="88" h="88">
        <view x="c" y="14" w="39" h="34" style="cloud_sun"/>
        <label name="day" style="weather_now" x="c" y="50" w="54" h="17" text="18~24"/>
        <label style="c" x="74" y="50" w="10" h="17" text="Â°"/>
      </button>
      <button name="watch_04_music" style="enable" enable_long_press="true" x="154" y="208" w="82" h="82">
        <health_circle name="music_circle" x="c" y="m" w="82" h="82" width="8" only_big="true" color_b="#F33A63" value_b="0" max_b="100" smooth_end="false" smooth_start="false"/>
        <image x="c" y="m" w="36" h="38" image="ico_music" draw_type="scale_auto"/>
      </button>
      <button name="icon_app_wechart" x="80" y="261" w="30" h="30" style="icon_app_wechart"/>
      <button name="icon_app_phone" x="125" y="299" w="29" h="29" style="icon_app_phone"/>
      <button name="icon_app_run" x="177" y="310" w="29" h="31" style="icon_app_run"/>
      <button name="icon_app_training" x="232" y="300" w="35" h="25" style="icon_app_training"/>
      <button name="icon_app_heart" x="282" y="263" w="31" h="30" style="icon_app_heart"/>
      <guage_hour_pointer sensitive="false" name="hour" x="c" y="m" w="100%" h="100%" image="ico_watch_04_hour"/>
      <guage_minute_pointer sensitive="false" name="minute" x="c" y="m" w="100%" h="100%" image="ico_watch_04_minute"/>
      <guage_second_pointer sensitive="false" name="second" x="c" y="m" w="100%" h="100%" image="ico_watch_04_second"/>
    </guage>
  </gesture>
</window>