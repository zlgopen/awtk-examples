<system_bar>
  <style name="default">
    <normal bg_color="#132634" border_color="#000000"  border="bottom"/>
  </style>
</system_bar>

<label>
  <style name="title" text_align_h="left" font_size="24">
    <normal text_color="#ffffff" />
  </style>
</label>

<digit_clock>
  <style name="time" font_size="24">
    <normal text_color="#ffffff" />
  </style>
</digit_clock>

<button>
  <style name="close" icon="earth">
    <normal     icon="close_n"/>
    <pressed    icon="close_p"/>
    <over       icon="close_n"/>
    <disable    icon="close_d"/>
  </style>
</button>
