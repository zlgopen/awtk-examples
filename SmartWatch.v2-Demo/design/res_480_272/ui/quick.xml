<window x="c" y="m" w="240" h="240" name="quick" open_anim_hint="slide_up(easing=sin_inout)" style:normal:bg_color="#000000" style:normal:bg_image="ico_pay_background">
  <gesture name="gesture" x="c" y="m" w="240" h="240">
    <slide_view name="slide_view" x="c" y="m" w="100%" h="100%">
      <view name="view" x="0" y="0" w="100%" h="100%">
        <label name="label" x="c" y="33" w="131" h="31" style="text" text="支付宝付款码"/>
        <button name="alipay" x="c" y="77" w="188" h="96" repeat="0" enable_long_press="false" style="alipay" text=""/>
      </view>
      <view name="view" x="0" y="0" w="100%" h="100%">
        <label name="label" x="c" y="33" w="120" h="31" style="text" text="请靠近读卡器"/>
        <button name="gongshang" x="c" y="77" w="169" h="106" repeat="0" enable_long_press="false" style="gongshang" text=""/>
      </view>
    </slide_view>
    <slide_indicator_arc name="slide_indicator_arc" x="0" y="228" w="240" h="12" spacing="8"/>
  </gesture>
</window>