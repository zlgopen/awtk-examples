<window anim_hint="htranslate" tr_text="bar_title" x="0%" y="50" w="800" h="430" >
    <view x="346" y="7%" w="284" h="7%" layout="r1 c3 m4 s10">
        <image name="typeie" clickable="true">
            <label name="typeie_icon" y="m" w="10" h="10" style="typeie"/>
            <label x="14" y="m" w="80%" h="100%" text="data1" style="series_label"/>
        </image>
        <image name="dayas" clickable="true">
            <label name="dayas_icon" y="m" w="10" h="10" style="dayas"/>
            <label x="14" y="m" w="80%" h="100%" text="data2" style="series_label"/>
        </image>
        <image name="drean" clickable="true">
            <label name="drean_icon" y="m" w="10" h="10" style="drean"/>
            <label x="14" y="m" w="80%" h="100%" text="data3" style="series_label"/>
        </image>
    </view>
    <chart_view name="chartview" x="6%" y="13%" w="82%" h="80%">
        <x_axis name="x" axis_type="category" data="[Mon,Tues,Wed,Thur,Fri,Sat,Sun]"/>
        <y_axis name="y" axis_type="value" min="0" max="140" data="[0,20,40,60,80,100,120,140]"/>
        <bar_series name="s1" capacity="7" bar="style:s1" style="edge1"/>
        <bar_series name="s2" capacity="7" bar="style:s2" style="spacing"/>
        <bar_series name="s3" capacity="7" bar="style:s3" style="edge2"/>

    </chart_view>
    <view x="right" y="0" w="101" h="100%" style="seperate" />
    <view name="right_menu" x="right" y="0" w="100" h="100%" layout="r6 c1 m19 s18" style="function_view">
        <button name="new_graph" style="new"/>
        <view />
        <view />
        <view />
        <view />
        <button name="close" style="back"/>
    </view>
</window>
