    <disable text_color="#bdbdbd" bg_color="#f9f9f9" border_color="#e6e6e6" />
  </style>

  <style name="combobox_down" border_color="#cbcbcb">
    <normal icon="arrow_down_n"/>
    <pressed icon="arrow_down_p"/>
    <over icon="arrow_down_o"/>
  </style>

  <style name="spinbox_down" border_color="#cbcbcb" bg_color="#f7f7f7">
