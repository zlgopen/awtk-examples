<view w="100%" h="100%" name="round_menu" style="music_bg_color">
  <color_button name="music_state" style="music_loop" x="0" y="0" w="100%" h="80" l="60" yoffset="5" orientation_top="false" button="true" disg:custom="container"/>
  <label name="song_name" style="song_name_white_43" x="c" y="120" w="100%" h="25"/>
  <view x="c" y="180" w="250" h="98">
    <button name="prev" style="music_prev" x="0" y="m" w="68" h="68"/>
    <music_play_button name="play" style="play" x="c" y="m" w="98" h="98" line_width="6" start_angle="270" line_color="#CA11FF" disg:custom="container"/>
    <button name="next" style="music_next" x="right" y="m" w="68" h="68"/>
  </view>
  <row x="c" y="320" w="200" h="18">
    <image x="3" y="m" w="20" h="18" image="music_sound"/>
    <slider x="28" y="m" w="153" h="18" style="music_vol" name="vol" bar_size="12" sensitive="true" min="0" max="100"/>
  </row>
</view>