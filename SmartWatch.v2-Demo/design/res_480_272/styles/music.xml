<button>
  <style name="music_circle_all" bg_image="ico_music_top_pressed" icon="ico_musicmudel_03_pressed">
    <normal bg_color="#00000000" bg_image="ico_music_top" icon="ico_musicmudel_03_normal"/>
    <over/>
    <pressed/>
  </style>
  <style name="music_circle_one" bg_color="#00000000" border_color="#00000000" round_radius="4" text_color="#00000000">
    <normal bg_image="ico_music_top" icon="ico_musicmudel_02_normal"/>
    <disable bg_color="#F4F4F466" border_color="#C2C2C266" text_color="#44444466"/>
    <focused bg_color="#F4F4F4" border_color="#338FFF" text_color="#444444"/>
    <over bg_image="ico_music_top_pressed" icon="ico_musicmudel_02_pressed"/>
    <pressed bg_image="ico_music_top_pressed" icon="ico_musicmudel_02_pressed"/>
  </style>
  <style name="music_next" icon="ico_music_next_pressed">
    <normal bg_color="#00000000" icon="ico_music_next_normal"/>
    <over/>
    <pressed/>
  </style>
  <style name="music_pause" icon="ico_music_stop_pressed">
    <normal bg_color="#00000000" icon="ico_music_stop_normal"/>
    <over/>
    <pressed/>
  </style>
  <style name="music_play" icon="ico_music_play_pressed">
    <normal bg_color="#00000000" icon="ico_music_play_normal"/>
    <over/>
    <pressed/>
  </style>
  <style name="music_pre" icon="ico_music_back_pressed">
    <normal border_color="#00000000" icon="ico_music_back_normal"/>
    <over/>
    <pressed/>
  </style>
  <style name="music_rand" bg_image="ico_music_top_pressed" icon="ico_musicmudel_01_pressed">
    <normal bg_color="#00000000" bg_image="ico_music_top" icon="ico_musicmudel_01_normal"/>
    <over/>
    <pressed/>
  </style>
</button>
<label>
  <style name="song_name">
    <normal font_size="25" text_color="#FFFFFF"/>
    <disable text_color="#44444466"/>
  </style>
</label>
<slider>
  <style name="music_vol" bg_color="#999999" fg_color="#CA12FF" icon="rounded_n" round_radius="5">
    <normal/>
    <disable bg_color="#D8D8D866" fg_color="#338FFF66"/>
    <focused bg_color="#D8D8D8" fg_color="#338FFF" icon="rounded_o"/>
    <over icon="rounded_o"/>
    <pressed icon="rounded_p"/>
  </style>
</slider>