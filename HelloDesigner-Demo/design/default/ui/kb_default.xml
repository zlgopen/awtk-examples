        <button name="page:symnum" text="123" x="15%" w="15%" h="100%" style="highlight"/>
        <button repeat="300" name="," text="," x="30%" w="10%" h="100%"/>
        <button repeat="300" name="space" text="space" x="40%" w="-80%" h="100%"/>
        <button repeat="300" name="." text="." x="60%" w="10%" h="100%"/>
        <button name="page:chinese" text="EN" x="70%" w="15%" h="100%" style="highlight"/>
        <button name="action" text="return" x="85%" w="15%" h="100%" style="highlight"/>
      </view>
    </clip_view>
    <clip_view name="symnum" x="0" w="100%" h="100%">
      <view w="15%" h="100%">
        <view w="100%" h="75%" children_layout="default(r=4,c=1)">
          <button repeat="300" name="." text="."/>
          <button repeat="300" name="/" text="/"/>
          <button repeat="300" name="+" text="+"/>
          <button repeat="300" name="-" text="-"/>
        </view>
        <button name="page:symbol" text="@&amp;$" y="75%" w="100%" h="25%" style="highlight"/>
      </view>
      <view x="15%" w="-30%" h="100%" children_layout="default(r=4,c=3)">
        <button repeat="300" name="1" text="1"/>
        <button repeat="300" name="2" text="2"/>
        <button repeat="300" name="3" text="3"/>
        <button repeat="300" name="4" text="4"/>
        <button repeat="300" name="5" text="5"/>
        <button repeat="300" name="6" text="6"/>
        <button repeat="300" name="7" text="7"/>
        <button repeat="300" name="8" text="8"/>
        <button repeat="300" name="9" text="9"/>
        <button name="page:lower" text="abc" style="highlight"/>
        <button repeat="300" name="0" text="0"/>
        <button repeat="300" name="space" text="space" style="highlight"/>
      </view>
      <view x="85%" w="15%" h="100%" children_layout="default(r=4,c=1)">
        <button repeat="300" name="key:backspace" text="backspace" style="highlight"/>
        <button repeat="300" name="*" text="*"/>
        <button repeat="300" name="#" text="#"/>
        <button name="action" text="return" style="highlight"/>
      </view>
    </clip_view>
    <clip_view name="symbol" x="0" w="100%" h="100%">
      <view x="0" w="100%" h="25%" children_layout="default(r=1,c=10)">
        <button repeat="300" name="[" text="["/>
        <button repeat="300" name="]" text="]"/>
        <button repeat="300" name="{" text="{"/>
        <button repeat="300" name="}" text="}"/>
        <button repeat="300" name="(" text="("/>
        <button repeat="300" name=")" text=")"/>
        <button repeat="300" name="^" text="^"/>
        <button repeat="300" name="%" text="%"/>
        <button repeat="300" name="+" text="+"/>
        <button repeat="300" name="=" text="="/>
      </view>
      <view x="5%" y="25%" w="-10%" h="25%" children_layout="default(r=1,c=9)">
        <button repeat="300" name="_" text="_"/>
        <button repeat="300" name="\\" text="\\"/>
        <button repeat="300" name="|" text="|"/>
        <button repeat="300" name="~" text="~"/>
        <button repeat="300" name="<" text="<"/>
        <button repeat="300" name=">" text=">"/>
        <button repeat="300" name="$" text="$"/>
        <button repeat="300" name="@" text="@"/>
        <button repeat="300" name="`" text="`"/>
      </view>
      <view y="50%" w="100%" h="25%">
        <button name="page:upper" text="shift" w="15%" h="100%" style="highlight"/>
        <view x="15%" w="-30%" h="100%" children_layout="default(r=1,c=7)">
          <button repeat="300" name="#" text="#"/>
          <button repeat="300" name=";" text=";"/>
          <button repeat="300" name="?" text="?"/>
          <button repeat="300" name="!" text="!"/>
          <button repeat="300" name="'" text="'"/>
          <button repeat="300" name="&quot;" text="&quot;"/>
          <button repeat="300" name="&amp;" text="&amp;"/>
        </view>
        <button repeat="300" name="key:backspace" text="backspace" x="85%" w="15%" h="100%" style="highlight"/>
      </view>
      <view y="75%" w="100%" h="25%">
        <button name="page:lower" text="abc" w="15%" h="100%" style="highlight"/>
        <button name="page:symnum" text="123" x="15%" w="15%" h="100%" style="highlight"/>
        <button repeat="300" name="," text="," x="30%" w="10%" h="100%"/>
        <button repeat="300" name="space" text="space" x="40%" w="-80%" h="100%"/>
        <button repeat="300" name="." text="." x="60%" w="10%" h="100%"/>
        <button name="page:chinese" text="EN" x="70%" w="15%" h="100%" style="highlight"/>
        <button name="action" text="return"  x="85%" w="15%" h="100%" style="highlight"/>
      </view>
    </clip_view>
    <clip_view name="chinese" x="0" w="100%" h="100%">
      <view x="0" w="100%" h="25%" children_layout="default(r=1,c=10)">
        <button repeat="300" name="key:q" text="q"/>
        <button repeat="300" name="key:w" text="w"/>
        <button repeat="300" name="key:e" text="e"/>
        <button repeat="300" name="key:r" text="r"/>
        <button repeat="300" name="key:t" text="t"/>
        <button repeat="300" name="key:y" text="y"/>
        <button repeat="300" name="key:u" text="u"/>
        <button repeat="300" name="key:i" text="i"/>
        <button repeat="300" name="key:o" text="o"/>
        <button repeat="300" name="key:p" text="p"/>
      </view>
      <view x="5%" y="25%" w="-10%" h="25%" children_layout="default(r=1,c=9)">
        <button repeat="300" name="key:a" text="a"/>
        <button repeat="300" name="key:s" text="s"/>
        <button repeat="300" name="key:d" text="d"/>
        <button repeat="300" name="key:f" text="f"/>
        <button repeat="300" name="key:g" text="g"/>
        <button repeat="300" name="key:h" text="h"/>
        <button repeat="300" name="key:j" text="j"/>
        <button repeat="300" name="key:k" text="k"/>
        <button repeat="300" name="key:l" text="l"/>
      </view>
      <view y="50%" w="100%" h="25%">
        <button name="page:upper" text="shift" w="15%" h="100%" style="highlight"/>
        <view x="15%" w="-30%" h="100%" children_layout="default(r=1,c=7)">
          <button repeat="300" name="key:z" text="z"/>
          <button repeat="300" name="key:x" text="x"/>
          <button repeat="300" name="key:c" text="c"/>
          <button repeat="300" name="key:v" text="v"/>
          <button repeat="300" name="key:b" text="b"/>
          <button repeat="300" name="key:n" text="n"/>
          <button repeat="300" name="key:m" text="m"/>
        </view>
        <button repeat="300" name="key:backspace" text="backspace" x="85%" w="15%" h="100%" style="highlight"/>
      </view>
      <view y="75%" w="100%" h="25%">
        <button name="page:symbol" text="@&amp;$" w="15%" h="100%" style="highlight"/>
        <button name="page:symnum" text="123" x="15%" w="15%" h="100%" style="highlight"/>
        <button repeat="300" name="，" text="，" x="30%" w="10%" h="100%"/>
        <button repeat="300" name="space" text="space" x="40%" w="-80%" h="100%"/>
        <button repeat="300" name="。" text="。" x="60%" w="10%" h="100%"/>
        <button name="page:lower" text="中" x="70%" w="15%" h="100%" style="highlight"/>
        <button name="action" text="return" x="85%" w="15%" h="100%" style="highlight"/>
