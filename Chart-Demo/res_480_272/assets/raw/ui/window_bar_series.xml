<window anim_hint="htranslate" tr_text="bar_title">
    <view x="158" y="2%" w="220" h="10%" layout="r1 c3 m0 s30">
        <image name="typeie" clickable="true">
            <label name="typeie_icon" x="0" y="m" w="10" h="10" style="typeie"/>
            <label x="14" y="m" w="50" h="100%" text="data1" style="series_label"/>
        </image>
        <image name="dayas" clickable="true">
            <label name="dayas_icon" y="m" w="10" h="10" style="dayas"/>
            <label x="14" y="m" w="50" h="100%" text="data2" style="series_label"/>
        </image>
        <image name="drean" clickable="true">
            <label name="drean_icon" y="m" w="10" h="10" style="drean"/>
            <label x="14" y="m" w="50" h="100%" text="data3" style="series_label"/>
        </image>
    </view>
    <chart_view name="chartview" x="7%" y="14%" w="73.5%" h="79%" bar.margin="10" bar.spacing="4">
        <axis name="x" type="category" orientation="x" tick="{show:false}" split_line="{show:false}" label="{color:#a1d8ef, font_size:16}" line="{color:#294b63}" data="[Mon,Tues,Wed,Thur,Fri,Sat,Sun]"/>
        <axis name="y" type="value" orientation="y" min="0" max="140" tick="{color:#294b63}" split_line="{color:#213b4d}" label="{color:#a1d8ef, font_size:16}" line="{color:#294b63}" data="[140,120,100,80,60,40,20,0]"/>
        <bar_series name="s1" capacity="7" bar="{color:#69cf5c}"/>
        <bar_series name="s2" capacity="7" bar="{color:#36b3c3}"/>
        <bar_series name="s3" capacity="7" bar="{color:#327ad3}"/>
    </chart_view>
    <view x="right" y="0" w="58" h="100%" style="seperate"/>
    <view name="right_menu" x="right" y="0" w="57" h="100%" layout="r6 c1 m10 s10" style="function_view">
        <button name="new_graph" style="new"/>
        <view />
        <view />
        <view />
        <view />
        <button name="close" style="back"/>
    </view>
</window>
