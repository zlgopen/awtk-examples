<dialog self_layout="default(x=480,y=30,w=240,h=201)" animation="move(x_from=480, x_to=240, duration=500)" style="bg_setting">
    <view x="0" y="0" w="100%" h="10%" style="dialog_title_gray">
        <button name="close" x="right:12" y="0" w="17" h="20" style="s_setting_close"/>
        <label x="9" y="0" w="20%" h="100%" tr_text="Advanced" style="title"/>
    </view>

    <view name="guage_pointer_view" x="0" y="10%" w="100%" h="100%">
        <view name="guage_low" x="20%" y="10%" w="72" h="72">
            <label x="left" y="middle:20" w="10" h="10" text="-"/>
            <label x="right" y="middle:20" w="10" h="10" text="+"/>
            <label x="center" y="bottom" w="10" h="10" tr_text="Low"/>
            <progress_circle name="circle_low" w="72" h="72" max="360" style="blue" start_angle="150" value="120" line_width="2"/>
            <guage_pointer x="10" y="10" name="low_guage_pointer" w="52" h="52" image="round"/>
        </view>
        <view name="guage_high" x="60%" y="10%" w="72" h="72">
            <label x="left" y="middle:20" w="10" h="10" text="-"/>
            <label x="right" y="middle:20" w="10" h="10" text="+"/>
            <label x="center" y="bottom" w="10" h="10" tr_text="High"/>
            <progress_circle name="circle_high" w="72" h="72" max="360" style="blue" start_angle="150" value="120" line_width="2" />
            <guage_pointer x="10" y="10" name="high_guage_pointer" w="52" h="52" image="round"/>
        </view>
        <view name="guage_round" x="20%" y="50%" w="72" h="72">
            <label x="left" y="middle:20" w="10" h="10" text="-"/>
            <label x="right" y="middle:20" w="10" h="10" text="+"/>
            <label x="center" y="bottom" w="10" h="10" tr_text="SURR-Strenth"/>
            <progress_circle name="circle_round" w="72" h="72" max="360" style="blue" start_angle="150" value="120" line_width="2"/>
            <guage_pointer x="10" y="10" name="round_guage_pointer" w="52" h="52" image="round"/>
        </view>
        <view name="guage_deep" x="60%" y="50%" w="72" h="72">
            <label x="left" y="middle:20" w="10" h="10" text="-"/>
            <label x="right" y="middle:20" w="10" h="10" text="+"/>
            <label x="center" y="bottom" w="10" h="10" tr_text="SURR-Depth"/>
            <progress_circle name="circle_deep" w="72" h="72" max="360" style="blue" start_angle="150" value="120" line_width="2"/>
            <guage_pointer x="10" y="10" name="deep_guage_pointer" w="52" h="52" image="round"/>
        </view>
    </view>
</dialog>