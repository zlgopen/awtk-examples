    <pressed icon="arrow_down_p"/>
    <over icon="arrow_down_o"/>
    <disable icon="arrow_down_d"/>
  </style>

  <style name="spinbox_up" border_color="#cbcbcb" bg_color="#f7f7f7">
    <normal icon="arrow_up_n"/>
    <pressed icon="arrow_up_p"/>
    <over icon="arrow_up_o"/>
    <disable icon="arrow_up_d"/>
  </style>

  <style name="spinbox_left" border_color="#cbcbcb" bg_color="#f7f7f7">
    <normal icon="arrow_left_n"/>
    <pressed icon="arrow_left_p"/>
    <over icon="arrow_left_o"/>
    <disable icon="arrow_left_d"/>
  </style>

  <style name="spinbox_right" border_color="#cbcbcb" bg_color="#f7f7f7">
    <normal icon="arrow_right_n"/>
    <pressed icon="arrow_right_p"/>
    <over icon="arrow_right_o"/>
    <disable icon="arrow_right_d"/>
  </style>

  <style name="scroll_down" border_color="#a0a0a0">
    <normal bg_color="#f0f0f0" icon="arrow_down_n"/>
    <pressed bg_color="#c0c0c0" icon="arrow_down_p"/>
