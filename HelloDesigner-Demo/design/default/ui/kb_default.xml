<keyboard theme="keyboard" x="0" y="bottom" w="100%" h="40%">
  <view name="candidates" x="0" w="100%" h="15%">
    <candidates x="0" w="100%" h="100%"/>
    <button name="close" x="right" w="16" h="100%" text="X" style="close"/>
  </view>
  <pages name="panel" x="0" y="bottom" w="100%" h="-16%" active="0">
    <clip_view name="lower" x="0" w="100%" h="100%">
      <view x="0" w="100%" h="25%" children_layout="default(r=1,c=10)">
        <button repeat="300" name="q" text="q"/>
        <button repeat="300" name="w" text="w"/>
        <button repeat="300" name="e" text="e"/>
        <button repeat="300" name="r" text="r"/>
        <button repeat="300" name="t" text="t"/>
        <button repeat="300" name="y" text="y"/>
        <button repeat="300" name="u" text="u"/>
        <button repeat="300" name="i" text="i"/>
        <button repeat="300" name="o" text="o"/>
        <button repeat="300" name="p" text="p"/>
      </view>
      <view x="5%" y="25%" w="-10%" h="25%" children_layout="default(r=1,c=9)">
        <button repeat="300" name="a" text="a"/>
        <button repeat="300" name="s" text="s"/>
        <button repeat="300" name="d" text="d"/>
        <button repeat="300" name="f" text="f"/>
        <button repeat="300" name="g" text="g"/>
        <button repeat="300" name="h" text="h"/>
        <button repeat="300" name="j" text="j"/>
        <button repeat="300" name="k" text="k"/>
        <button repeat="300" name="l" text="l"/>
      </view>
      <view y="50%" w="100%" h="25%">
        <button name="page:upper" text="shift" w="15%" h="100%" style="highlight"/>
        <view x="15%" w="-30%" h="100%" children_layout="default(r=1,c=7)">
          <button repeat="300" name="z" text="z"/>
          <button repeat="300" name="x" text="x"/>
          <button repeat="300" name="c" text="c"/>
          <button repeat="300" name="v" text="v"/>
          <button repeat="300" name="b" text="b"/>
          <button repeat="300" name="n" text="n"/>
          <button repeat="300" name="m" text="m"/>
        </view>
        <button repeat="300" name="key:backspace" text="backspace" x="85%" w="15%" h="100%" style="highlight"/>
      </view>
      <view y="75%" w="100%" h="25%">
        <button name="page:symbol" text="@&amp;$" w="15%" h="100%" style="highlight"/>
        <button name="page:symnum" text="123" x="15%" w="15%" h="100%" style="highlight"/>
        <button repeat="300" name="," text="," x="30%" w="10%" h="100%"/>
        <button repeat="300" name="space" text="space" x="40%" w="-80%" h="100%"/>
        <button repeat="300" name="." text="." x="60%" w="10%" h="100%"/>
        <button name="page:chinese" text="EN" x="70%" w="15%" h="100%" style="highlight"/>
        <button name="action" text="return"  x="85%" w="15%" h="100%" style="highlight"/>
      </view>
    </clip_view>
    <clip_view name="upper" x="0" w="100%" h="100%">
      <view x="0" w="100%" h="25%" children_layout="default(r=1,c=10)">
        <button repeat="300" name="Q" text="Q"/>
        <button repeat="300" name="W" text="W"/>
        <button repeat="300" name="E" text="E"/>
        <button repeat="300" name="R" text="R"/>
        <button repeat="300" name="T" text="T"/>
        <button repeat="300" name="Y" text="Y"/>
        <button repeat="300" name="U" text="U"/>
        <button repeat="300" name="I" text="I"/>
        <button repeat="300" name="O" text="O"/>
